{
  "name": "asteliria-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:web": "expo build:web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "analyze:bundle": "npx expo export --platform web --dev false && npx bundlesize",
    "optimize:images": "imagemin assets/images/* --out-dir=assets/images/optimized",
    "performance:check": "node scripts/performance-check.js",
    "performance:images": "node scripts/optimize-images.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "expo-apple-authentication": "~7.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-unused-imports": "^1.0.7",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "bundlesize": "^0.18.1",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2"
  },
  "bundlesize": [
    {
      "path": "./dist/static/js/*.js",
      "maxSize": "500 kB",
      "compression": "gzip"
    },
    {
      "path": "./dist/static/css/*.css",
      "maxSize": "50 kB",
      "compression": "gzip"
    }
  ],
  "private": true
}
